{
	"name": "deptinternalaffairsnz/cwp-installer",
	"description": "A SilverStripe installer for CWP",
	"require": {
		"php": ">=5.3.2",
		"cwp/cwp-core": "^1.0",
		"silverstripe/cms": "^3.1",
		"silverstripe/framework": "^3.1"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"config": {
		"process-timeout": 600
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.cwp.govt.nz/"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev"
}
